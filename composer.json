{
  "name": "silvercommerce/customisable-products",
  "description": "SilverCommerce module that adds customisable products (adding dropdowns, radio buttons or textfields to a product)",
  "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/silvercommerce/customisable-products",
    "keywords": [
        "silverstripe",
        "silvercommerce",
        "commerce",
        "product",
        "customisable"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Mo",
            "email": "morven@ilateral.co.uk"
        }
    ],
    "support": {
        "issues": "https://github.com/silvercommerce/customisable-products/issues"
    },
    "require": {
        "silvercommerce/catalogue-admin": "^1.1",
        "silvercommerce/shoppingcart": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "SilverCommerce\\CustomisableProducts\\": "src/"
        }
    },
    "suggest": {
        "silvercommerce/catalogue-frontend": "Render your product catalogue into the frontend of the site"
    }
}
